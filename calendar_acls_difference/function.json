{
  "bindings": [
    {
      "name": "aclsActual",
      "type": "blobTrigger",
      "direction": "in",
      "path": "calendar-acls/{filename}",
      "connection": "wrdsbigor_STORAGE"
    },
    {
      "name": "aclsIdeal",
      "type": "blob",
      "direction": "in",
      "path": "calendar-acls/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name": "aclsDiff",
      "type": "blob",
      "direction": "out",
      "path": "calendar-acls-differences/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name" : "$return",
      "type" : "serviceBus",
      "queueName" : "log_to_slack",
      "connection" : "wrdsb-babbage_SERVICEBUS",
      "accessRights" : "manage",
      "direction" : "out"
    }
  ],
  "disabled": false
}