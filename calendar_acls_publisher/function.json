{
  "bindings": [
    {
      "name": "aclsDiff",
      "type": "blobTrigger",
      "direction": "in",
      "path": "calendar-acls-differences/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name": "aclsDiffTopic",
      "type": "serviceBus",
      "topicName": "calendar_acls_differences",
      "subscriptionName": "wrdsb-babbage",
      "accessRights": "Manage",
      "connection": "wrdsb-babbage_SERVICEBUS",
      "direction": "out"
    },
    {
      "name" : "$return",
      "type" : "serviceBus",
      "queueName" : "log_to_slack",
      "connection" : "wrdsb-babbage_SERVICEBUS",
      "accessRights" : "manage",
      "direction" : "out"
    }
  ],
  "disabled": false
}
