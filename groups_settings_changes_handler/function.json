          "direction": "in",
          "path": "groups-settings-changes/{filename}",
          "connection": "wrdsbbabbage_STORAGE"
        },
        {
          "name": "groupsSettingsNow",
          "type": "blob",
          "direction": "in",
          "path": "groups-settings/{filename}",
          "connection": "wrdsbcodex_STORAGE"
        },
        {
          "name": "groupsSettingsPrevious",
          "type": "blob",
