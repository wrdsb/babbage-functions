{
  "bindings": [
    {
      "name": "membershipsActual",
      "type": "blobTrigger",
      "direction": "in",
      "path": "member-list/{filename}",
      "connection": "wrdsbigor_STORAGE"
    },
    {
      "name": "membershipsIdeal",
      "type": "blob",
      "direction": "in",
      "path": "groups-memberships/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name": "membershipsSupplemental",
      "type": "blob",
      "direction": "out",
      "path": "groups-memberships-supplemental/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name": "membershipsSupplementalAll",
      "type": "blob",
      "direction": "in",
      "path": "groups-memberships-supplemental/all.json",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name": "membershipsDiff",
      "type": "blob",
      "direction": "out",
      "path": "groups-memberships-differences/{filename}",
      "connection": "wrdsbbabbage_STORAGE"
    },
    {
      "name" : "$return",
      "type" : "serviceBus",
      "queueName" : "log_to_slack",
      "connection" : "wrdsb-babbage_SERVICEBUS",
      "accessRights" : "manage",
      "direction" : "out"
    }
  ],
  "disabled": false
}